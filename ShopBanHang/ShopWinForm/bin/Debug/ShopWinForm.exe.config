<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IServiceUser" />
                <binding name="BasicHttpBinding_IServiceSanPham" />
                <binding name="BasicHttpBinding_IServiceDanhMucSP" />
                <binding name="BasicHttpBinding_IServiceSanPhamCustom" />
                <binding name="BasicHttpBinding_IServiceNhapHang" />
                <binding name="BasicHttpBinding_IServiceDatHang" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:57383/ServiceUser.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IServiceUser" contract="ServiceReferenceUser.IServiceUser"
                name="BasicHttpBinding_IServiceUser" />
            <endpoint address="http://localhost:57383/ServiceSanPham.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServiceSanPham"
                contract="ServiceReferenceSanPham.IServiceSanPham" name="BasicHttpBinding_IServiceSanPham" />
            <endpoint address="http://localhost:57383/ServiceDanhMucSP.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServiceDanhMucSP"
                contract="ServiceReferenceDanhMucSP.IServiceDanhMucSP" name="BasicHttpBinding_IServiceDanhMucSP" />
            <endpoint address="http://localhost:57383/ServiceSanPhamCustom.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServiceSanPhamCustom"
                contract="ServiceSanPhamCustom.IServiceSanPhamCustom" name="BasicHttpBinding_IServiceSanPhamCustom" />
            <endpoint address="http://localhost:57383/ServiceNhapHang.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServiceNhapHang"
                contract="ServiceNhapHang.IServiceNhapHang" name="BasicHttpBinding_IServiceNhapHang" />
            <endpoint address="http://localhost:57383/ServiceDatHang.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServiceDatHang"
                contract="ServiceDatHang.IServiceDatHang" name="BasicHttpBinding_IServiceDatHang" />
        </client>
    </system.serviceModel>
</configuration>